{
  "compilerOptions": {
    "paths": {
      "@environments/*": [
        "src/environments/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@pipes/*": [
        "src/app/pipes/*"
      ],
      "@pages/*": [
        "src/app/pages/*"
      ],
      "@ui/*": [
        "src/app/ui/*"
      ]
    }
  }
}
